{
  "version": 2,
  "builds": [
    {
      "src": "askaprilai/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/askaprilai/askapril-replica.html"
    },
    {
      "src": "/askaprilai/(.*)",
      "dest": "/askaprilai/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/askaprilai/$1"
    }
  ]
}