{
  "version": 2,
  "builds": [
    {
      "src": "askapril-replica.html",
      "use": "@vercel/static"
    },
    {
      "src": "ai-copilot-backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/copilot/(.*)",
      "dest": "/ai-copilot-backend.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}